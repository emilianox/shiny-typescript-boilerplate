{
    "defaultSeverity": "warning",
    "extends": [
        "tslint:latest",
        "tslint-config-airbnb",
        "tslint-sonarts",
        "tslint-react"
        // "./.tslint/tslint.cleancode.json",
        // "./.tslint/tslint.inmutable.json"
    ],
    "jsRules": {},
    "rules": {
        "import-name": [
            true,
            {
                "react": "React"
            }
        ],
        "variable-name": [
            true,
            "ban-keywords",
            "check-format",
            "allow-pascal-case"
        ],
        "jsx-no-multiline-js": false,
        "no-null-keyword": true,
        "object-literal-sort-keys": false,
        "align": false
    },
    "rulesDirectory": []
}