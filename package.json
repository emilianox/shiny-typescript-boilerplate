{
  "name": "ts-base",
  "version": "1.0.0",
  "description": "A very opinionated React/Typescript Base Web Project.",
  "scripts": {
    "analize-build": "npm run clean && webpack --progress --colors --mode=production --config .webpack/webpack.prod.analize.js",
    "build": "npm run clean && webpack --progress --colors --mode=production --config .webpack/webpack.config.js",
    "build-storybook": "build-storybook",
    "clean": "rimraf dist/**",
    "start": "webpack-dev-server --config .webpack/webpack.config.js --progress --colors --open --mode=development",
    "storybook": "start-storybook -p 6006",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "type-coverage": "type-coverage --detail"
  },
